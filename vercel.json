{
    "builds": [
      {
        "src": "fullstack/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      }
    ],
    "routes": [
    {
      "src": "/static/(.*)",
      "dest": "fullstack-django/fullstack/static/$1"  
    },
    {
      "src": "/(.*)",
      "dest": "fullstack-django/fullstack/server.py" 
    }
  ]
  }
  